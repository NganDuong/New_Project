    <script src="{{ asset('bundles/framework/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/bootstrap.min.js') }}"></script>
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/bootstrap.min.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/owl.carousel.css') }}" />
    <script src="{{ asset('bundles/framework/js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/owl.carousel.js') }}"></script>

    {# include handed-code file #}
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/header.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/nav.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/banner.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/right.bar.css') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/content.css') }}" />
